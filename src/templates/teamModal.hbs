<div class='content'>
    <button class='close'></button>   
    {{#each this}}
    <figure class='team-card'>
        <img src='{{photo}}' alt={{fullName}} class='profile' />
        <figcaption>
            <h3 class="team-name">{{fullName}}<span>{{position}}</span></h3>
            <div class='icons'>
                <a href='{{linkedIn}}' target="_blank" rel="noopener noreferrer">
                    <i class='ion-social-linkedin-outline'></i></a>
                <a href='{{gitHub}}' target="_blank" rel="noopener noreferrer"> 
                    <i class='ion-social-github-outline'></i></a>
                    <a href='{{telegram}}' target="_blank" rel="noopener noreferrer">
                        <i class='ion-paper-airplane'></i></a>
            </div>
        </figcaption>
    </figure>
    {{/each}}
</div>