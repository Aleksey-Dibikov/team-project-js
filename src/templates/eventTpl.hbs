{{#each this}}
<li class="events__item">
   <a href="" class="link events__link" target="_blank" rel="noreferrer noopener">
      <div class="events__image-wrap">
         <picture>
            <img src="{{ this.images.[3].url}}" alt="" title="" class="events__image" id = {{this.id}} />
            {{!-- дописала id в img, чтобы брать событие - Люда--}}
         </picture>
      </div>
      <div class="events__descr">
         <h3 class="events__name" id = {{this.id}}>{{this.name}}</h3>
         <p class="events__date" id = {{this.id}}>{{this.dates.start.localDate}}</p>
         <p class="events__location" id = {{this.id}}>
            <svg class="events__location-icon" width="16" height="12">
               <use href="./images/svg/symbol-defs.svg#icon-map"></use>
            </svg>
         {{this._embedded.venues.[0].name}} </p>
      </div>
   </a>
</li>
{{/each}}